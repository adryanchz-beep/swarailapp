# Redirect any PHP file to homepage
/*.php / 301

# Force HTTPS and remove www
http://swarailapk.com/* https://swarailapk.com/:splat 301!
https://www.swarailapk.com/* https://swarailapk.com/:splat 301!

# Remove .html extension
/:slug.html /:slug/ 301

# Remove index.html except homepage
/*/index.html /:splat/ 301

# Redirect ALL 404 pages to homepage
/* / 301
